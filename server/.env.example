PORT=3000
FRONTEND_URL=http://localhost:5173

MONGODB_URI=your_mongodb_uri
JWT_SECRET=your_jwt_secret

# Stripe (required for payments) – use test keys for local testing
# Dashboard: https://dashboard.stripe.com/test/apikeys
STRIPE_SECRET_KEY=sk_test_...

# Stripe webhook (recommended for production – reliable booking creation even if user closes tab)
# In Stripe Dashboard: Developers → Webhooks → Add endpoint → URL: https://your-api.com/api/payment/webhook → Event: checkout.session.completed
STRIPE_WEBHOOK_SECRET=whsec_...

# ImageKit (optional)
IMAGEKIT_PUBLIC_KEY=
IMAGEKIT_PRIVATE_KEY=
IMAGEKIT_URL_ENDPOINT=

# Tesla Fleet API (OAuth – connect owner account to fetch all vehicles)
# https://developer.tesla.com/docs/fleet-api
# Create app: https://developer.tesla.com/ → get Client ID & Secret
# Redirect URI must match exactly (e.g. https://api.mongoori.com/api/tesla/callback)
TESLA_CLIENT_ID=
TESLA_CLIENT_SECRET=
TESLA_REDIRECT_URI=
TESLA_FLEET_API_BASE=https://fleet-api.prd.na.vn.cloud.tesla.com
# Region registration (required once per region): run server/scripts/register-tesla-region.js
# Domain must match allowed_origins root; host EC public key at https://<domain>/.well-known/appspecific/com.tesla.3p.public-key.pem
TESLA_PARTNER_DOMAIN=
# Partner token (Fleet/Partner approved): use this so charging sessions & telemetry work for all owners without each owner connecting Tesla. Operator logs in → data is served to owners/drivers.
# TESLA_PARTNER_ACCESS_TOKEN=
# TESLA_ACCESS_TOKEN=  # alternative env name for same partner token

# Fastrak / Tolls (optional – daily toll sync)
FASTRAK_API_KEY=
